import random
choices = ['Rock','Paper','Scissors']
player_choice = input

while True:
    player_choice = input("Choose 'Rock','Paper', or 'Scissors'").lower()
    comp_choice = random.choice(choices)

    if player_choice == comp_choice:
        print("It's a tie!")
        print(comp_choice)
    elif player_choice == 'paper' and comp_choice == 'rock':
        print('Winner, Winner Chicken Dinner')
        print(comp_choice)
    elif player_choice == 'rock' and comp_choice == 'scissors':
        print('Winner, Winner Chicken Dinner')
        print(comp_choice)
    elif player_choice == 'scissors' and comp_choice == 'paper':
        print('Winner, Winner Chicken Dinner')
        print(comp_choice)
    elif player_choice != 'Scissors' or player_choice != 'Paper' or player_choice != 'Rock' or player_choice != 'scissors' or player_choice != 'paper' or player_choice != 'rock':
        print('invalid')
    else:
        print('You lost.')


